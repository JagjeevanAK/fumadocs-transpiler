{
  "name": "fumadocs-transpiler",
  "version": "1.2.0",
  "description": "A transpiler that converts annotated Markdown files to Fuma-docs compatible React components",
  "main": "dist/index.js",
  "bin": {
    "fumadocs-transpiler": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build"
  },
  "keywords": [
    "markdown",
    "transpiler",
    "fumadocs",
    "react",
    "documentation",
    "cli",
    "mdx",
    "annotations",
    "components"
  ],
  "author": "Jagjeevan Kashid",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jagjeevanak/fumadocs-transpiler.git"
  },
  "homepage": "https://github.com/jagjeevanak/fumadocs-transpiler#readme",
  "bugs": {
    "url": "https://github.com/jagjeevanak/fumadocs-transpiler/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "chokidar": "^3.5.3",
    "gray-matter": "^4.0.3",
    "remark": "^15.0.1",
    "remark-parse": "^11.0.0",
    "unified": "^11.0.4",
    "unist-util-visit": "^5.0.0",
    "chalk": "^5.3.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/fs-extra": "^11.0.4",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.8",
    "ts-jest": "^29.1.1"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
